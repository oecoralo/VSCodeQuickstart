<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>PromptAction.TimesActionTaken:AVG</calculatedFormula>
        <datatype>percent</datatype>
        <developerName>FORMULA1</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Percent of Users who Took Custom Action</masterLabel>
        <scale>0</scale>
    </aggregates>
    <columns>
        <field>PromptAction$User</field>
    </columns>
    <columns>
        <field>PromptAction$User.UserRole</field>
    </columns>
    <columns>
        <field>PromptAction$LastResult</field>
    </columns>
    <columns>
        <field>PromptAction$LastResultDate</field>
    </columns>
    <columns>
        <field>PromptAction$DisplayType</field>
    </columns>
    <description>Identify the percent of users who clicked the custom action button, grouped by user profile</description>
    <filter>
        <criteriaItems>
            <column>PromptAction$DisplayType</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>contains</operator>
            <value>Docked,Floating</value>
        </criteriaItems>
    </filter>
    <format>Summary</format>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>PromptAction$User.Profile</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>PromptAction$Name</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <name>Prompt Percent Custom Action by Profile</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>In_App_Guidance_User_Data_Prompt_Dasboard_V1__c</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <showGrandTotal>true</showGrandTotal>
    <showSubTotals>true</showSubTotals>
    <timeFrameFilter>
        <dateColumn>PromptAction$PublishedDate</dateColumn>
        <interval>INTERVAL_CUSTOM</interval>
    </timeFrameFilter>
</Report>
