<?xml version="1.0" encoding="UTF-8"?>
<Report xmlns="http://soap.sforce.com/2006/04/metadata">
    <aggregates>
        <calculatedFormula>PromptAction.TimesActionTaken:AVG</calculatedFormula>
        <datatype>percent</datatype>
        <description>What percent of users took the custom action?</description>
        <developerName>FORMULA1</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Percent of Users who Took Custom Action</masterLabel>
        <scale>2</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>PromptAction.User.Name:UNIQUE</calculatedFormula>
        <datatype>number</datatype>
        <developerName>FORMULA2</developerName>
        <isActive>true</isActive>
        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Total Unique Users Interacting</masterLabel>
        <scale>0</scale>
    </aggregates>
    <columns>
        <field>PromptAction$LastResultDate</field>
    </columns>
    <columns>
        <field>PromptAction$User</field>
    </columns>
    <columns>
        <field>PromptAction$User.Profile</field>
    </columns>
    <columns>
        <aggregateTypes>Sum</aggregateTypes>
        <field>PromptAction$TimesDisplayed</field>
    </columns>
    <columns>
        <aggregateTypes>Sum</aggregateTypes>
        <field>PromptAction$TimesDismissed</field>
    </columns>
    <columns>
        <aggregateTypes>Sum</aggregateTypes>
        <field>PromptAction$TimesActionTaken</field>
    </columns>
    <columns>
        <field>PromptAction$LastDisplayDate</field>
    </columns>
    <columns>
        <field>PromptAction$PublishedDate</field>
    </columns>
    <columns>
        <field>PromptAction$DisplayType</field>
    </columns>
    <description>Identify user engagement including the percent of the time users clicked the custom action button, grouped by prompt</description>
    <filter>
        <criteriaItems>
            <column>PromptAction$DisplayType</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>contains</operator>
            <value>Docked,Floating</value>
        </criteriaItems>
        <criteriaItems>
            <column>PromptAction$PublishedDate</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>false</isUnlocked>
            <operator>equals</operator>
            <value>LAST_N_DAYS:3000</value>
        </criteriaItems>
    </filter>
    <format>Summary</format>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>PromptAction$Name</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <groupingsDown>
        <dateGranularity>Day</dateGranularity>
        <field>PromptAction$LastResult</field>
        <sortOrder>Asc</sortOrder>
    </groupingsDown>
    <name>Overall Engagement by Prompt</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>In_App_Guidance_User_Data_Prompt_Dasboard_V1__c</reportType>
    <scope>organization</scope>
    <showDetails>true</showDetails>
    <showGrandTotal>true</showGrandTotal>
    <showSubTotals>true</showSubTotals>
    <timeFrameFilter>
        <dateColumn>PromptAction$PublishedDate</dateColumn>
        <interval>INTERVAL_CUSTOM</interval>
    </timeFrameFilter>
</Report>
