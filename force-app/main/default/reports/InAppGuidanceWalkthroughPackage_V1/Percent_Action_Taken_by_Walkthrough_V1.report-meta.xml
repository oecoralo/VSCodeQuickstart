    </groupingsDown>
    <name>Percent Action Taken by Walkthrough</name>
    <params>
        <name>co</name>
        <value>1</value>
    </params>
    <reportType>In_App_Guidance_with_User_Data_WT_Package_V1__c</reportType>
    <scope>organization</scope>
